{
  "name": "@tt/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "imports": {
    "#app/*": "./src/*",
    "#app/generated/prisma/client": "./src/generated/prisma/client.ts",
    "#prisma/seed/*": "./prisma/seed/*"
  },
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "import": "./src/client/index.ts"
    },
    "./constants": {
      "import": "./src/constants/index.ts"
    },
    "./enums": {
      "import": "./src/generated/prisma/enums/*"
    },
    "./models": {
      "import": "./src/generated/prisma/models/*"
    },
    "./types": {
      "import": "./src/types/index.ts"
    },
    ".": {
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "db:clean": "bunx rimraf dev.db prisma/migrations",
    "db:migrate": "bunx prisma migrate dev",
    "db:generate": "bunx prisma generate",
    "db:seed": "bun prisma/seed/seed.ts",
    "db": "bun run db:clean && bun run db:generate && bun run db:migrate && bun run db:seed"
  },
  "dependencies": {
    "@prisma/adapter-libsql": "7.4.2",
    "@prisma/client": "7.4.2",
    "@prisma/driver-adapter-utils": "7.4.2",
    "@tt/core": "workspace:*",
    "async-mutex": "^0.5.0",
    "js-base64": "^3.7.8",
    "promise-limit": "^2.7.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/bun": "1.3.9",
    "prisma": "7.4.2"
  },
  "peerDependencies": {
    "@typescript/native-preview": "^7.0.0-dev.20260301.1"
  },
  "trustedDependencies": [
    "@prisma/client",
    "@prisma/engines",
    "prisma"
  ]
}
